    <% if app.liked_by?(current_user)%>
      <%=link_to "💗", app_likes_path(app.id), data: { turbo_method: :delete }, class: "app_likes float", remote: true %>
      <%= app.likes.count %>
    <% else %>
      <%=link_to "🤍", app_likes_path(app.id), data: { turbo_method: :post }, class: "app_likes float", remote: true %>
    <%= app.likes.count %>
    <% end %>